               <lb/>mit] <hi rendition="#aq">aus</hi> durch <hi rendition="#aq">
                  <hi rendition="#i">H</hi>
               </hi>
               <hi rendition="#rkd">20</hi> weniger] <hi rendition="#aq">davor gestr.</hi> mir <hi rendition="#aq">
                  <hi rendition="#i">H</hi>
               </hi>
               <hi rendition="#rkd">24</hi> die dramatische] <hi rendition="#aq">aus</hi>
