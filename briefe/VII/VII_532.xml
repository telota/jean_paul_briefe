               <hi rendition="#b">
                  <hi rendition="#aq"/>
                  <hi rendition="#aq">270</hi>
               </hi>
               <hi rendition="#aq">,</hi>
               <hi rendition="#rkd">
                  <hi rendition="#aq">17</hi>
               </hi>
               <lb/> Silbergrube <hi rendition="#aq">
