      <body>
         <div type="letter">
            <note type="ig">Kopie</note>
            <dateline>
               <hi rendition="#right">
                  <metamark>[</metamark>
                  <placeName>Bayreuth</placeName>, <date>7. Mai 1816</date>
                  <metamark>]</metamark>
               </hi>
