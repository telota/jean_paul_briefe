                  <hi rendition="#i">dri Madera</hi>] nachtr.</hi>
               <hi rendition="#rkd">33</hi> ändern] <hi rendition="#aq">aus</hi> bessern<lb/>
               <hi rendition="#b"><hi rendition="#aq">113</hi></hi><hi rendition="#aq">,</hi><hi rendition="#rkd"><hi rendition="#aq">5</hi></hi>
