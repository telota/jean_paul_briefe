              </valList>
            </attDef>
            <attDef ident="type" mode="change">
              <valList type="closed" mode="change">
                <valItem mode="add" ident="baseText"/>
                <valItem mode="add" ident="witnessText"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>  
        
